<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="250dp"
        android:layout_height="170dp"
        android:layout_marginEnd="30dp"
        app:layout_constraintBottom_toTopOf="@+id/logojspi"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.082"
        app:srcCompat="@drawable/jslogorainbow" />

    <ImageView
        android:id="@+id/logojspi"
        android:layout_width="250dp"
        android:layout_height="250dp"
        android:layout_marginTop="80dp"
        android:layout_marginBottom="82dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView10"
        app:layout_constraintEnd_toEndOf="@+id/imageView2"
        app:layout_constraintHorizontal_bias="0.464"
        app:layout_constraintStart_toStartOf="@+id/imageView2"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/jslogo"
        tools:srcCompat="@drawable/jslogo" />

    <ImageView
        android:id="@+id/bottomlogos"
        android:layout_width="600dp"
        android:layout_height="50dp"
        android:layout_marginStart="80dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/circlebottom"
        app:srcCompat="@drawable/bottomlogo" />

    <ImageView
        android:id="@+id/circleshadowtop"
        android:layout_width="500dp"
        android:layout_height="500dp"
        android:layout_marginBottom="-20dp"
        app:layout_constraintBottom_toBottomOf="@+id/circletop"
        app:layout_constraintEnd_toEndOf="@+id/circletop"
        app:srcCompat="@drawable/recurso2" />

    <ImageView
        android:id="@+id/circletop"
        android:layout_width="500dp"
        android:layout_height="500dp"
        android:layout_marginEnd="-300dp"
        android:layout_marginBottom="-150dp"
        app:layout_constraintBottom_toTopOf="parent"
        app:layout_constraintEnd_toStartOf="parent"
        app:srcCompat="@drawable/circle" />

    <ImageView
        android:id="@+id/circlebottom"
        android:layout_width="450dp"
        android:layout_height="450dp"
        android:layout_marginTop="-150dp"
        android:layout_marginEnd="-200dp"
        app:layout_constraintEnd_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="parent"
        app:srcCompat="@drawable/circle" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="300dp"
        android:layout_height="50dp"
        app:layout_constraintBottom_toTopOf="@+id/bottomlogos"
        app:layout_constraintEnd_toStartOf="@+id/logojspi"
        app:layout_constraintStart_toEndOf="@+id/circleshadowtop"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/recurso5" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="300dp"
        android:layout_height="80dp"
        android:background="#19FDFDFD"
        app:layout_constraintBottom_toBottomOf="@+id/imageView7"
        app:layout_constraintEnd_toEndOf="@+id/imageView7"
        app:layout_constraintStart_toStartOf="@+id/imageView7"
        app:srcCompat="@drawable/title1" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="450dp"
        android:layout_height="50dp"
        app:layout_constraintEnd_toEndOf="@+id/imageView7"
        app:layout_constraintStart_toStartOf="@+id/imageView7"
        app:layout_constraintTop_toBottomOf="@+id/imageView8"
        app:srcCompat="@drawable/title2" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="440dp"
        android:layout_height="50dp"
        android:layout_marginStart="-15dp"
        android:layout_marginTop="4dp"
        app:layout_constraintBottom_toTopOf="@+id/bottomlogos"
        app:layout_constraintStart_toEndOf="@+id/circlebottom"
        app:layout_constraintTop_toBottomOf="@+id/imageView9"
        app:srcCompat="@drawable/title3" />


    <ImageButton
        android:id="@+id/btn_ingreso"
        android:layout_width="160dp"
        android:layout_height="55dp"
        android:background="#00FFFFFF"

        android:scaleType="centerInside"
        app:layout_constraintBottom_toBottomOf="@+id/imageView10"
        app:layout_constraintStart_toEndOf="@+id/imageView10"
        app:layout_constraintTop_toTopOf="@+id/imageView10"
        app:srcCompat="@drawable/btn_clic" />

    <ImageView
        android:id="@+id/click_image"
        android:layout_width="65dp"
        android:layout_height="72dp"
        android:layout_marginStart="-16dp"
        app:layout_constraintStart_toEndOf="@+id/btn_ingreso"
        app:layout_constraintTop_toTopOf="@+id/btn_ingreso"
        app:srcCompat="@drawable/clicarrow" />


</androidx.constraintlayout.widget.ConstraintLayout>